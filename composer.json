{
  "name": "kain/think-redis",
  "description": "ThinkPHP Redis 扩展",
  "require": {
    "ext-json": "*",
    "php": ">=7.4.0",
    "topthink/framework": "^6.0.0",
    "predis/predis": "^1.1"
  },
  "require-dev": {
    "roave/security-advisories": "dev-master",
    "phpunit/phpunit": "8"
  },
  "autoload": {
    "psr-4": {
      "think\\redis\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "RedisTests\\": "library/think-redis/tests/"
    }
  },
  "extra": {
    "think": {
      "services": [
        "think\\redis\\service\\RedisService"
      ]
    }
  },
  "authors": [
    {
      "name": "kain",
      "email": "zhangtqx@vip.qq.com"
    }
  ],
  "support": {
    "docs": "https://think-bit.kainonly.com",
    "email": "zhangtqx@vip.qq.com"
  },
  "license": "MIT"
}
